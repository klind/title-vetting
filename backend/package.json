{
  "name": "title-company-vetter-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:production": "tsc && npx playwright install --with-deps chromium",
    "start": "node dist/server/server.js",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "dev": "tsx src/server/dev-server.ts",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "playwright:install": "npx playwright install --with-deps chromium"
  },
  "dependencies": {
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsdom": "^24.0.0",
    "node-fetch": "^3.3.2",
    "playwright": "^1.48.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.26.2",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.150",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^22.10.12",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
